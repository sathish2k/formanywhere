{
    "name": "@formanywhere/form-runtime",
    "version": "0.0.1",
    "type": "module",
    "sideEffects": ["**/*.css", "**/*.scss"],
    "main": "./src/index.tsx",
    "types": "./src/index.tsx",
    "exports": {
        ".": {
            "solid": "./src/index.tsx",
            "import": "./src/index.tsx",
            "types": "./src/index.tsx"
        },
        "./styles": "./src/styles.scss",
        "./preview": {
