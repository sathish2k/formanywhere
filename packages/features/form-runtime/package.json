    "dependencies": {
        "@formanywhere/domain": "workspace:*",
        "@formanywhere/ui": "workspace:*",
        "@formanywhere/shared": "workspace:*",
        "@modular-forms/solid": "0.25.1",
        "zod": "3",
        "solid-js": "^1.9.11"
    },
    "devDependencies": {
        "typescript": "^5.7.0"
    },
    "peerDependencies": {
        "solid-js": "^1.9.0",
        "@modular-forms/solid": ">=0.25.0",
        "zod": ">=3"
    }
