{
    "name": "@formanywhere/ui",
    "version": "0.0.1",
    "type": "module",
    "sideEffects": [
        "**/*.css",
        "**/*.scss"
    ],
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "solid": "./src/index.ts",
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./*": {
            "solid": "./src/*/index.tsx",
            "import": "./src/*/index.tsx",
            "types": "./src/*/index.tsx"
        },
        "./styles/*": "./src/styles/*"
    },
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "lint": "echo 'No linter configured'"
    },
    "dependencies": {
        "@floating-ui/dom": "^1.6.12",
        "solid-js": "^1.9.11"
    },
    "devDependencies": {
        "sass": "1.97.3",
        "sass-embedded": "1.97.3",
        "typescript": "^5.7.0"
    },
    "peerDependencies": {
        "solid-js": "^1.9.0"
    }
}