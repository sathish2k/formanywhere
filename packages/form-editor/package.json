{
    "name": "@formanywhere/form-editor",
    "version": "0.0.1",
    "type": "module",
    "sideEffects": [
        "**/*.css",
        "**/*.scss"
    ],
    "main": "./src/index.tsx",
    "types": "./src/index.tsx",
    "exports": {
        ".": {
            "solid": "./src/index.tsx",
            "import": "./src/index.tsx",
            "types": "./src/index.tsx"
        },
        "./styles": "./src/styles.scss",
        "./ai": {
            "solid": "./src/components/ai/AIFormBuilder.tsx",
            "import": "./src/components/ai/AIFormBuilder.tsx",
            "types": "./src/components/ai/AIFormBuilder.tsx"
        },
        "./import": {
            "solid": "./src/components/import/ImportForm.tsx",
            "import": "./src/components/import/ImportForm.tsx",
            "types": "./src/components/import/ImportForm.tsx"
        },
        "./layout": {
            "solid": "./src/components/layout/FormEditorLayout.tsx",
            "import": "./src/components/layout/FormEditorLayout.tsx",
            "types": "./src/components/layout/FormEditorLayout.tsx"
        },
        "./schema": {
            "import": "./src/engine/schema.ts",
            "types": "./src/engine/schema.ts"
        }
    },
    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "lint": "echo 'No linter configured'"
    },
    "dependencies": {
        "@formanywhere/ui": "workspace:*",
        "@formanywhere/shared": "workspace:*",
        "solid-js": "^1.9.11"
    },
    "devDependencies": {
        "typescript": "^5.7.0"
    },
    "peerDependencies": {
        "solid-js": "^1.9.0"
    }
}