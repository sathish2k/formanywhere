    "scripts": {
        "dev": "tsc --watch",
        "build": "tsc",
        "lint": "echo 'No linter configured'"
    },
    "dependencies": {
        "@formanywhere/ui": "workspace:*",
        "better-auth": "1.4.18",
        "solid-js": "^1.9.11"
    },
    "peerDependencies": {
        "solid-js": "^1.9.0",
        "@formanywhere/form-editor": "workspace:*",
        "@formanywhere/form-runtime": "workspace:*"
