{
    "name": "@formanywhere/shared",
    "version": "0.0.1",
    "type": "module",
    "sideEffects": [
        "**/*.css",
        "**/*.scss"
    ],
    "main": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./feature-tabs": {
            "import": "./src/feature-tabs/index.tsx",
            "types": "./src/feature-tabs/index.tsx"
        },
        "./use-cases-grid": {
            "import": "./src/use-cases-grid/index.tsx",
            "types": "./src/use-cases-grid/index.tsx"
        },
        "./types": {
            "import": "./src/types/index.ts",
            "types": "./src/types/index.ts"
        },
        "./utils": {
            "import": "./src/utils/index.ts",
            "types": "./src/utils/index.ts"
        },
        "./constants": {
            "import": "./src/constants/index.ts",
            "types": "./src/constants/index.ts"
        },
        "./header": {
            "import": "./src/components/header/index.tsx",
            "types": "./src/components/header/index.tsx"
        },
        "./footer": {
            "import": "./src/components/footer/index.tsx",
            "types": "./src/components/footer/index.tsx"
        },
        "./auth-client": {
            "import": "./src/lib/auth-client.ts",
            "types": "./src/lib/auth-client.ts"
        },
        "./confirmation-dialog": {
            "import": "./src/components/confirmation-dialog/index.tsx",
            "types": "./src/components/confirmation-dialog/index.tsx"
        },
        "./form-setup": {
            "import": "./src/components/form-setup/index.ts",
            "types": "./src/components/form-setup/index.ts"
        },
        "./form-setup/layout-builder": {
            "import": "./src/components/form-setup/layout-builder/LayoutBuilder.tsx",
            "types": "./src/components/form-setup/layout-builder/LayoutBuilder.tsx"
        },
        "./form-setup/layout-builder/types": {
            "import": "./src/components/form-setup/layout-builder/types.ts",
            "types": "./src/components/form-setup/layout-builder/types.ts"
        },
        "./icons/svg/menu-hamburger.svg": {
            "import": "./src/icons/svg/menu-hamburger.svg",
