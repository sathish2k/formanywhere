        },
        "./icons/svg/github.svg": {
            "import": "./src/icons/svg/github.svg",
            "default": "./src/icons/svg/github.svg"
        },
        "./components/cookie-consent": {
            "solid": "./src/components/cookie-consent.tsx",
            "import": "./src/components/cookie-consent.tsx",
            "types": "./src/components/cookie-consent.tsx"
        },
        "./templates-browser": {
            "import": "./src/components/form-setup/templates-browser.tsx",
            "types": "./src/components/form-setup/templates-browser.tsx"
        },
        "./templates-api": {
            "import": "./src/lib/templates-api.ts",
            "types": "./src/lib/templates-api.ts"
        }
